# ============================================
# Forminator Export Formats - .gitignore
# ============================================

# Dependencies (never commit these)
/vendor/
/node_modules/

# Build outputs
/build/
*.zip

# Composer files (dev only)
composer.lock
.composer

# Development/testing
.phpunit.result.cache
phpunit.xml
.php-cs-fixer.cache
*.log
*.cache
coverage/
.phpunit.cache/

# IDE/Editor
.idea/
.vscode/
*.sublime-*
*.swp
*.swo
*~

# OS files
.DS_Store
Thumbs.db
Desktop.ini
._*

# Security - never commit these
*.pem
*.key
*.crt
.env
.env.local
*.sql
*.bak

# Temporary files
*.tmp
*.temp
/tmp/

